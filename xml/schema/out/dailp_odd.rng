   <define name="seg">
      <element name="seg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(arbitrary segment) represents any segmentation of text below the chunk level. [16.3. Blocks, Segments, and Anchors 6.2. Components of the Verse Line 7.2.5. Speech Contents]</a:documentation>
         <ref name="macro.paraContent"/>
         <ref name="att.global.attributes"/>
         <ref name="att.segLike.attributes"/>
         <ref name="att.typed.attribute.subtype"/>
         <ref name="att.written.attributes"/>
         <ref name="att.notated.attributes"/>
         <optional>
            <attribute name="type">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <choice>
                  <value>idea_unit</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(sentence-level) Indicates that the segment is a single sentence-level unit in the source transcription</a:documentation>
                  <value>trans</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(sentence-level) Indicates that the segment is a single sentence-level unit in the free translation</a:documentation>
                  <value>romanization</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a romanization of the word</a:documentation>
                  <value>simple_phonetics</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a simple phonetics representation of the word</a:documentation>
                  <value>phonemic_form</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a phonemic representation of the word</a:documentation>
                  <value>morphemic_segmentation</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a morphemic segmentation of the word</a:documentation>
                  <value>morphemic_gloss</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a morpheme-by-morpheme gloss of the word</a:documentation>
                  <value>gloss</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(word-level) Indicates that the segment is a gloss of the word</a:documentation>
               </choice>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
